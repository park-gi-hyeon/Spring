<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<!-- 	탭에 두번째에 있는 Nameespaces에 c와p부분을 체크하면 자동 생성 -->

	<bean id="student3" class="com.lgy.spring_6_1.Student">
		<constructor-arg value="홍길자"></constructor-arg>
		<constructor-arg value="8"></constructor-arg>
		<constructor-arg>
			<list>
				<value>줄넘기</value>
				<value>공기놀이</value>
			</list>
		</constructor-arg>
		<property name="height" value="126"></property>
		<property name="weight" value="21"></property>
	</bean>
	
	<bean id="family" class="com.lgy.spring_6_1.Family" c:papaName="홍아빠" c:mamName="홍엄마" p:sisterName="홍누나">
	 	<property name="brotherName" value="홍오빠"></property>
	</bean>
</beans>
